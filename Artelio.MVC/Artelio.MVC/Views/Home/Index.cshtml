@{
    Layout = null;
}

@model PageHomeDTO


<!DOCTYPE html>
<html lang="zxx">

<head>

    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <!-- grid css -->
    <link rel="stylesheet" href="~/assets/main/css/plugins/bootstrap-grid.css">
    <!-- font awesome css -->
    <link rel="stylesheet" href="~/assets/main/css/plugins/fontawesome.min.css">
    <!-- swiper css -->
    <link rel="stylesheet" href="~/assets/main/css/plugins/swiper.min.css">
    <!-- okai css -->
    <link rel="stylesheet" href="~/assets/main/css/style-stylish.css">
    <!-- page title -->
    <title>Artelio</title>

</head>

<body>

    <!-- wrapper -->
    <div id="smooth-wrapper" class="mil-page-wrapper">

        <!-- cursor -->
        <div class="mil-cursor-follower"></div>
        <!-- cursor end -->
        <!-- preloader -->
        <div class="mil-preloader">
            <div class="mil-preloader-animation">
                <div class="mil-pos-abs mil-animation-1">
                    <p class="mil-head1 mil-m1">Pioneering</p>
                    <p class="mil-head1 mil-a2">Creative</p>
                    <p class="mil-head1 mil-m1">Excellence</p>
                </div>
                <div class="mil-pos-abs mil-animation-2">
                    <div class="mil-reveal-frame">
                        <p class="mil-reveal-box"></p>
                        <p class="mil-head1 mil-m1">Artelio.com</p>
                    </div>
                </div>
            </div>
        </div>
        <!-- preloader end -->
        <!-- scroll progress -->
        <div class="mil-progress-track">
            <div class="mil-progress"></div>
        </div>
        <!-- scroll progress end -->
        <!-- fixed elements -->
        <div class="mil-fixed">
            <div class="mil-top-panel-2" style="background: linear-gradient(0deg, rgb(51 51 51) 0%, rgba(0, 0, 0, 0) 31%);">
                <div class="container">
                    <div class="mil-left-side mil-tp-transition" id="swupTpLeft">
                        <a href="~/assets/main/#top" class="mil-logo mil-scroll-to" data-no-swup>
                            <i class="far fa-cube"></i>
                            <span>Artelio</span>
                        </a>
                    </div>

                    <div class="mil-onepage-nav" id="swup-opm">
                    </div>

                    <div class="mil-buttons-tp-frame mil-c-gone">
                        <div class="mil-buttons">
                            <a href="~/assets/main/contact-stl.html" class="mil-tp-btn"><i class="fal fa-envelope"></i></a>
                            <a asp-action="Profile" asp-controller="Profile" class="mil-tp-btn">
                                <div>
                                    <img src="~/@Model.GetUserInfoDTO.ImageUrl" alt="Alternate Text" style="border-radius: 100%; width:40px; height: 40px;" />
                                </div>
                            </a>
                            <div class="mil-tp-btn">
                                <div class="mil-menu-btn"><span></span></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="mil-menu-frame mil-menu-frame-2">
                <div class="mil-menu-window">
                    <div class="container">
                        <div class="row mil-no-g">
                            <div class="col-lg-6">
                                <div class="mil-left-side">
                                    <div class="mil-menu-part mil-inner-scroll" id="swupMenu">
                                        <ul class="mil-main-menu mil-c-gone">
                                            <li>
                                                <a asp-action="Index" asp-controller="Home">Home</a>

                                            </li>

                                            <li>
                                                <a asp-action="Profile" asp-controller="Profile">Profile</a>

                                            </li>
                                            <li>
                                                <a asp-action="Index" asp-controller="Profile">Settings</a>

                                            </li>
                                            
                                        </ul>
                                    </div>
                                    <div class="mil-bottom-part">
                                        <p class="mil-text-sm">©2024. All rights reserved.</p>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="mil-right-side">
                                    <div class="mil-links-part">
                                        <ul class="mil-links">
                                            <li><a asp-controller="profile" asp-action="index">Basic Info</a></li>
                                            <li><a asp-controller="profile" asp-action="socialmedia">Social Media</a></li>
                                            <li><a asp-controller="profile" asp-action="workexperience">Work Experience</a></li>
                                            <li><a asp-controller="profile" asp-action="award">Award</a></li>
                                            <li><a asp-controller="profile" asp-action="language">Language</a></li>
                                            <li><a asp-controller="profile" asp-action="skills">Skill</a></li>



                                        </ul>
                                    </div>
                                    <div class="mil-blog-part">
                                        <div class="mil-blog-section">
                                            <div class="mil-jcb mil-aic">
                                                <h4 class="mil-head4 mil-mb30">Your project</h4>
                                                <div class="mil-sb-nav mil-mb30">
                                                    <div class="mil-slider-btn mil-sb-prev mil-c-gone"><i class="fal fa-arrow-left"></i></div>
                                                    <div class="mil-slider-btn mil-sb-next mil-c-gone"><i class="fal fa-arrow-right"></i></div>
                                                </div>
                                            </div>
                                            <div class="swiper-container mil-blog-slider-sm">
                                                <div class="swiper-wrapper">
                                                    @foreach (var item in Model.getAllUserProjectDTOs)
                                                    {
                                                        <div class="swiper-slide">
                                                            <a asp-action="Detail" asp-controller="Home" asp-route-project="@item.Id" class="mil-blog-card-sm mil-c-gone">
                                                                <div class="mil-cover">
                                                                    <div class="mil-hover-frame">
                                                                        <img src="~/@item.ImageUrl" alt="cover">
                                                                    </div>
                                                                </div>
                                                                <div class="mil-text-frame">
                                                                    <h4 class="mil-head6 mil-max-1row-text">@item.ProjectName</h4>
                                                                </div>
                                                            </a>
                                                        </div>
                                                    }

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- fixed elements end -->
        <!-- page transition -->
        <div class="mil-transition-fade" id="swup">
            <div class="mil-transition-frame">

                <!-- content -->
                <div id="smooth-content" class="mil-content">

                    <!-- hero -->
                    <div class="mil-hero-1 mil-sm-hero mil-stl mil-up" id="top">
                        <div class="mil-overlay"></div>
                        <div class="container mil-hero-main mil-relative mil-aic">
                            <div class="mil-hero-text mil-scale-img" data-value-1="1.3" data-value-2="0.95">
                                <div class="mil-text-pad"></div>
                                <h1 class="mil-display2 mil-rubber">Our best <span class="mil-a2">works</span></h1>
                            </div>
                        </div>
                    </div>
                    <!-- hero end -->
                    <!-- portfolio -->
                    <div class="mil-p-0-130">
                        <div class="container-fluid">
                            <div class="row">
                                <div style="display: flex; flex-wrap: wrap; justify-content: center;">
                                    @foreach (var item in Model.getAllProjects)
                                    {
                                        <div class="col-md-4" style="margin: 2%; width: 25%">
                                            <div class="mil-work-card mil-stl mil-mb30">
                                                <div class="mil-cover mil-port mil-up">
                                                    <div class="mil-hover-frame">
                                                        <img src="~/@item.ImageUrl" alt="cover" class="mil-scale-img" data-value-1="1.15" data-value-2="1">
                                                    </div>
                                                </div>
                                                <div class="mil-hover-overlay">
                                                    <div class="mil-descr">
                                                        <a asp-action="profile" asp-controller="profile" asp-route-userId="@item.UserId" style="text-align:center;">
                                                            <img src="~/@item.UserImageUrl" alt="Alternate Text" style="border-radius: 100%; width:40px; height:40px;" />
                                                            <h1 style="font-family:Arial, Helvetica, sans-serif; font-size:10px;">@item.Fullname</h1>
                                                        </a>
                                                        <a asp-action="detail" asp-controller="home" asp-route-project="@item.Id" class="mil-descr">
                                                            <div class="mil-text-frame" style="padding:5px;">
                                                                <h4 class="mil-head4 mil-max-1row-text mil-m1 mil-c-gone">@item.ProjectName</h4>
                                                            </div>
                                                            <div class="mil-768-gone mil-c-gone">
                                                                <div class="mil-stylized-btn mil-a1">
                                                                    <i class="fal fa-arrow-up"></i>
                                                                    <span>Read more</span>
                                                                </div>
                                                            </div>
                                                        </a>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- portfolio end -->



                </div>
                <!-- content -->

            </div>
        </div>
        <!-- page transition -->

    </div>
    <!-- wrapper end -->
    <!-- swup js -->
    <script src="~/assets/main/js/plugins/swup.min.js"></script>
    <!-- gsap js -->
    <script src="~/assets/main/js/plugins/gsap.min.js"></script>
    <!-- scroll smoother -->
    <script src="~/assets/main/js/plugins/ScrollSmoother.min.js"></script>
    <!-- scroll trigger js -->
    <script src="~/assets/main/js/plugins/ScrollTrigger.min.js"></script>
    <!-- scroll to js -->
    <script src="~/assets/main/js/plugins/ScrollTo.min.js"></script>
    <!-- swiper js -->
    <script src="~/assets/main/js/plugins/swiper.min.js"></script>
    <!-- parallax js -->
    <script src="~/assets/main/js/plugins/parallax.js"></script>

    <!-- pixy js -->
    <script src="~/assets/main/js/main.js"></script>

</body>

</html>
