                                const fullText = el.dataset.fulltext || '';
                                const remainingLength = fullText.length - initialLength;
                                const halfChunk = Math.ceil(remainingLength / 2);

                                let step = 0;

                                const span = document.createElement("span");

                                span.textContent = fullText.slice(0, initialLength);
                                el.appendChild(span);
                                if (fullText.length > initialLength) {
                                    const toggle = document.createElement("span");
                                    toggle.className = "toggle";
                                    toggle.textContent = "...";
                                    el.appendChild(toggle);

                                    toggle.onclick = () => {
                                        step++;
                                        if (step === 1) {
                                            span.textContent = fullText.slice(0, initialLength + halfChunk);
                                        } else if (step === 2) {
                                            span.textContent = fullText;
                                            toggle.textContent = " (show less)";
                                        } else {
                                            span.textContent = fullText.slice(0, initialLength);
                                            toggle.textContent = "...";
                                            step = 0;
                                        }
                                    };
                                }
                            });
                        });
                    </script>


                </div>
                <!-- /WIDGET BOX -->
                        }

