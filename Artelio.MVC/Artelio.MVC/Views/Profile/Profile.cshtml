          <p class="user-short-description-text"><a>@Model.GetUserProfileDTO.Username</a></p>
          <!-- /USER SHORT DESCRIPTION TEXT -->
        </div>
        <!-- /USER SHORT DESCRIPTION -->
  
  
        <!-- USER STATS -->
